<h1>
  <a href="/" style="color: black; text-decoration: none;">
    Edinburgh
  </a>
</h1>
<div>
  <p>geo info "from Edinburgh, Scotland, United Kingdom" goes with</p>
  <% if logged_in? %>
    <textarea type="text" id="text" name="text" placeholder="text" cols="24" rows="10"></textarea>
    <button type="submit" id="goButton" style="width: 50px; height: 50px;">go</button>
    <p id="message"><%= @message %></p>
    <div>
      <a href="/logout">logout</a>
    </div>
  <% else %>
    <div>
      <a href="/auth/twitter">login</a>
    </div>
  <% end %>
</div>
<script src="/js/index.js"></script>

