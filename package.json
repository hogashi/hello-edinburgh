  "scripts": {
    "build-dev": "run-p build:js-dev copy",
    "build": "run-p build:js copy",
    "heroku-postbuild": "run-s build",
    "build:js-dev": "webpack --mode development --config webpack.config.js",
    "build:js": "webpack --mode production --config webpack.config.js",
    "build:css": "node-sass src/index.scss public/css/index.css",
    "copy": "mkdir -p public && cp -rf static/* public/",
    "watch": "webpack --mode development --config webpack.config.js --watch",
    "clean": "rm -rf public",
    "lint": "tslint -c tslint.json src/**/*.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "axios": "^0.18.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "typescript": "^3.3.3"
  }
